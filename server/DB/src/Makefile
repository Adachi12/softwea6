submain:
	gcc -Wall -o ../../bin/calorie.o -c calorie.c
	gcc -Wall -o ../../bin/saved_route.o -c saved_route.c
	gcc -Wall -o ../../bin/usedlog.o -c usedlog.c
	gcc -Wall -o ../../bin/user.o -c user.c

# カロリーテーブル
calorie.o: calorie.c
	gcc -Wall -o calorie.o -c calorie.c

# 保存したルートテーブル
saved_route.o: saved_route.c
	gcc -Wall -o saved_route.o -c saved_route.c

# 利用履歴テーブル
usedlog.o: usedlog.c
	gcc -Wall -o usedlog.o  -c usedlog.c

# ユーザテーブル
user.o: user.c
	gcc -Wall -o user.o -c user.c